{
  "name": "fissionjs",
  "version": "0.9.2",
  "description": "A simple to use and extendable policy driven JS object schema library",
  "main": "./lib/index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm test -- -w",
    "document": "./node_modules/.bin/esdoc",
    "clean": "rm -rf lib",
    "build": "tsc --emitDeclarationOnly && babel src --out-dir lib --extensions \".ts\"",
    "pretest": "npm run build",
    "test": "mocha --colors --recursive \"./src/**/*.test.js\" ts-node/register src/**/test.ts",
    "test:js": "mocha --colors --recursive \"./src/**/*.test.js\"",
    "lint": "eslint src/**/*.{js} --ext .js"
  },
  "files": [
    "lib"
  ],
  "repository": "git+https://github.com/sackrin/fissionjs.git",
  "author": "Hilary Osborne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sackrin/fissionjs/issues"
  },
  "homepage": "https://github.com/sackrin/fissionjs#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.52",
    "@babel/core": "^7.0.0-beta.52",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.52",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.52",
    "@babel/preset-env": "^7.0.0-beta.52",
    "@babel/preset-typescript": "^7.0.0-beta.52",
    "@types/chai": "^4.1.4",
    "@types/lodash": "^4.14.110",
    "@types/mocha": "^5.2.4",
    "@types/validatorjs": "^3.7.1",
    "chai": "^4.1.2",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^5.1.1",
    "typescript": "^2.9.2",
    "typings": "^2.1.1"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "validatorjs": "^3.14.2"
  }
}

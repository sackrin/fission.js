    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^5.1.1"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "validatorjs": "^3.14.2"
